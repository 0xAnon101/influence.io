{
  "connection": "default",
  "collectionName": "campaign",
  "info": {
    "name": "campaign",
    "description": "Customer website campaign for the website."
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "campaignName": {
      "type": "string",
      "multiple": false
    },
    "websiteName": {
      "type": "string",
      "multiple": false
    },
    "websiteUrl": {
      "type": "string",
      "multiple": false,
      "unique": true
    },
    "utmSource": {
      "type": "string",
      "multiple": false
    },
    "utmMedium": {
      "type": "string",
      "multiple": false
    },
    "trackingId": {
      "type": "string",
      "multiple": false
    },
    "isActive": {
      "type": "boolean",
      "multiple": false
    },
    "state": {
      "type": "string",
      "multiple": false
    },
    "profile": {
      "model": "profile",
      "via": "campaign"
    },
    "notificationTypes": {
      "model": "notificationtypes",
      "via": "campaign"
    },
    "configuration": {
      "model": "configuration",
      "via": "campaign"
    },
    "rule": {
      "model": "rules",
      "via": "campaign"
    }
  }
}