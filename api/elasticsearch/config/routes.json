  {
  "routes": [
    {
      "method": "GET",
      "path": "/es/health",
      "handler": "Elasticsearch.health",
      "config": {
        "policies": [
          "plugins.users-permissions.permissions"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/es/search",
      "handler": "Elasticsearch.query",
      "config": {
        "policies": [
          "plugins.users-permissions.permissions"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/es/search/:_id",
      "handler": "Elasticsearch.Notification",
      "config": {
        "policies": [
          "plugins.users-permissions.permissions"
        ]
      }
    }
  ]
}
